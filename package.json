{
  "name": "sequelize-pg-bulkupdate",
  "version": "1.0.0",
  "description": "Sequelize add-on to allow optiomized bulk update on postgresql",
  "main": "index.js",
  "scripts": {
    "dev": "yarn dev:test",
    "test": "yarn run jasmine",
    "dev:test": "yarn run nodemon --exec \"yarn test\""
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*",
      ".git/*"
    ],
    "ext": "js,json,yml,yaml",
    "delay": "500"
  },
  "keywords": [
    "sequelize",
    "postgresql",
    "pg",
    "bulk"
  ],
  "author": "Christian Roy <christian@roy.name>",
  "license": "ISC",
  "dependencies": {
    "sequelize": "^4.42.0"
  },
  "devDependencies": {
    "jasmine": "^3.3.1",
    "nodemon": "^1.18.10",
    "pg": "^7.8.0",
    "uuid": "^3.3.2"
  }
}
